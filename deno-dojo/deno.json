{
  "name": "@gendojo/deno-dojo",
  "version": "0.0.2",
  "license": "Apache-2.0",
  "exports": {
    ".": "./src/mod.ts"
  },
  "publish": {
    "include": [
      "src/**/*.ts",
      "catalog/**/*.ts",
      "catalog/**/*.json",
      "catalog/**/*.md",
      "deno.json",
      "Dockerfile",
      "*.sh",
      "README.md"
    ],
    "exclude": [
      "src/**/*_test.*"
    ]
  },
  "imports": {
    "@std/cli": "jsr:@std/cli@^1.0.6",
    "@std/fs": "jsr:@std/fs@^1.0.4",
    "@std/path": "jsr:@std/path@^1.0.6"
  },
  "tasks":{
    "dev:init": "deno init foo123",
    "dev:todo-100": "deno run -NRW src/mod.ts todo-100 foo123",
    "dev:test": "cd foo123; deno task dojo:test",
    "jsr:todo-100": "deno run -NRW jsr:@gendojo/deno-dojo todo-100 foo123"
  }
}
